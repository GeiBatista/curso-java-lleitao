package streams;

import java.util.Arrays;
import java.util.List;

public class Filter {

	public static void main(String[] args) {
		
		Aluno a1 = new Aluno("Luna", 7.8);
		Aluno a2 = new Aluno("Geisi", 5.8);
		Aluno a3 = new Aluno("Luiza", 9.8);
		Aluno a4 = new Aluno("Vini", 6.8);
		Aluno a5 = new Aluno("Gei", 7.1);
		Aluno a6 = new Aluno("Binha", 8.8);
		
		List<Aluno> alunos = Arrays.asList(a1, a2, a3, a4, a5, a6);
		
		
	}
}
